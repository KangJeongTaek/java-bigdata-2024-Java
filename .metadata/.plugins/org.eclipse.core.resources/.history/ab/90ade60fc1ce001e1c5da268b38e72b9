package KangJeongTaek.day14.P704;

public class LambdaExample {
	public static void main(String[] args) {
		Person person = new Person();
		person.action((x,y) ->{
			double result = x + y;
			return result;
		});
		
		person.action((x,y) ->  (x+y));


		public staic double sum(double x, double y) {
			return (x+y);
		}
		
	}
}
