package KangJeongTaek.day13.Thread.P697;

public class LambdaExample {
	public static void main(String[] args) {
		action((x,y) -> {
			int result = x + y;
			System.out.println(result);
		});
		
		action((x,y)->{
			int result = x-y;
			System.out.println(result);
			return result;
		});
		
		
	}
	public static void action(Calculable calculabe) {
		int x =10;
		int y =4;
		
		calculabe.calculate(x, y);
	}
}
